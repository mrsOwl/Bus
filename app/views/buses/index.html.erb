<p id="notice"><%= notice %></p>

<h1>Buses</h1>

<table>
  <thead>
    <tr>
      <th>Bus Number</th>
      <th>Tickets Places</th>
      <th>Bus station</th>
      <th>Bus time start</th>
      <th>Bus time stop</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @buses.each do |bus| %>
      <tr>
        <td><%= bus.number %></td>
         <td><%= bus.tickets_places %></td>
        <td><%= bus.bus_station %></td>
        <td><%= bus.bus_time_start %></td>
        <td><%= bus.bus_time_stop %></td>
        <td><%= link_to 'Show', bus %></td>
        <td><%= link_to 'Edit', edit_bus_path(bus) %></td>
        <td><%= link_to 'Destroy', bus, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bus', new_bus_path %>
<%= link_to 'New Ticket', new_ticket_path %>
